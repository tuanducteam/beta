# If you have any plugins, put them here!
gem 'github-pages', '>= 0.1.0' if Gem.win_platform?
gem 'jekyll-admin', group: :jekyll_plugins
group :jekyll_plugins do
    gem 'jekyll-feed'
    gem 'jekyll-sitemap'
    gem 'jekyll-paginate'
    gem 'jekyll-archives'
    gem 'jekyll-seo-tag'
    gem 'kramdown'
    gem 'rouge'
    gem 'jekyll-compress-images'
    gem 'jekyll-webp'
end
